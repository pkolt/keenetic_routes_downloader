{
  "$schema": "https://json.schemastore.org/chrome-manifest",
  "name": "Keenetic Routes Downloader",
  "description": "Позволяет скачать таблицу маршрутизации для роутеров Keenetic",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["downloads", "tabs", "webNavigation"],
  "host_permissions": ["http://192.168.1.1/*"],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    }
  }
}
